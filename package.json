{
  "name": "caixa",
  "productName": "Caixa",
  "version": "1.0.0",
  "description": "Sistema Loja",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "release": "build",
    "publish": "build -p always",
    "test": "build --ia32 "
  },
  "build": {
    "appId": "florestalferragens",
    "productName": "Florestal Ferragens",
    "extraFiles": [
      "config/"
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "publish": {
      "provider": "github",
      "owner": "VZiemer",
      "repo": "caixa",
      "token": "9c090373c25ef4d9c11dbd61fca7a46a65c9fa7d"
    },
    "win": {
      "target": [
        {
          "target": "nsis-web",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsisWeb": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "icon": "build/icon.ico"
  },
  "repository": "https://github.com/VZiemer/caixa.git",
  "keywords": [],
  "author": "VZiemer",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.8.4",
    "electron-builder": "^20.9.2",
    "electron-rebuild": "^1.7.3",
    "node-gyp": "^3.6.2",
    "rebuild": "^0.1.2"
  },
  "dependencies": {
    "calendario": "^1.1.4",
    "electron-config": "^1.0.0",
    "electron-updater": "^2.18.2",
    "ffi": "^2.2.0",
    "iconv": "^2.3.0",
    "ini": "^1.3.5",
    "md-data-table": "^2.2.0",
    "node-firebird": "^0.8.4",
